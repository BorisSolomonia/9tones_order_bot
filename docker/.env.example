# Copy this file to docker/.env and fill real values.

# Production image tags (used by compose.production.yml)
GCP_PROJECT_ID=your-gcp-project-id
TAG=latest

# Backend runtime
SPRING_PROFILES_ACTIVE=mock
LOG_LEVEL=DEBUG

# Google Sheets
GOOGLE_SHEETS_ENABLED=false
GOOGLE_SHEETS_CREDENTIALS_PATH=/secrets/service-account.json
GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id

# RS.GE
RSGE_ENABLED=true
RSGE_ENDPOINT=https://services.rs.ge/WayBillService/WayBillService.asmx
RSGE_SU=your-rsge-user
RSGE_SP=your-rsge-password
RSGE_TIMEOUT=120
RSGE_CONNECT_TIMEOUT_SECONDS=30
RSGE_CHUNK_DAYS=3
RSGE_CHUNK_PARALLELISM=2
RSGE_SOAP_NAMESPACE=http://tempuri.org/
RSGE_DATE_FORMAT=yyyy-MM-dd'T'HH:mm:ss
RSGE_DEBUG=false
RSGE_DEBUG_SAMPLE_COUNT=3
RSGE_DEBUG_RESPONSE_SNIPPET_LENGTH=0
RSGE_FULL_SYNC_MONTHS=2

# Telegram
TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN=your-bot-token
TELEGRAM_CHAT_ID=your-chat-id
TELEGRAM_API_BASE_URL=https://api.telegram.org
TELEGRAM_CONNECT_TIMEOUT_SECONDS=10
TELEGRAM_REQUEST_TIMEOUT_SECONDS=15
TELEGRAM_MESSAGE_DATE_FORMAT=dd.MM.yyyy
TELEGRAM_MESSAGE_ORDER_TITLE=Order
TELEGRAM_MESSAGE_MANAGER_LABEL=Manager
TELEGRAM_MESSAGE_DATE_LABEL=Date
TELEGRAM_MESSAGE_CUSTOMERS_LABEL=Customers
TELEGRAM_MESSAGE_TOTAL_LABEL=Total
TELEGRAM_MESSAGE_TOTAL_SUFFIX=customers

# App
APP_CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
APP_FLUSH_INTERVAL_SECONDS=5
APP_REFRESH_INTERVAL_SECONDS=300
APP_SECURITY_LOGIN_RATE_MAX_ATTEMPTS=5
APP_SECURITY_LOGIN_RATE_WINDOW_SECONDS=300
APP_SYNC_MAX_RETRIES=3
APP_SYNC_RETRY_DELAYS_MS=2000,4000,8000
APP_SYNC_SOURCE_USER=rsge_sync
APP_DRAFTS_WEEKDAY_NAMES=monday,tuesday,wednesday,thursday,friday,saturday,sunday

# Frontend
NEXT_PUBLIC_API_URL=
NEXT_PUBLIC_QUERY_STALE_TIME_MS=30000
NEXT_PUBLIC_QUERY_RETRY_COUNT=1
NEXT_PUBLIC_SEARCH_DEBOUNCE_MS=300
NEXT_PUBLIC_CUSTOMERS_PAGE_SIZE=1000
NEXT_PUBLIC_ADMIN_CUSTOMERS_PAGE_SIZE=1000
